{
    "datacenter": "local",
    "data_dir": "/var/consul",
    "ui_dir": "/var/consul/ui",
    "log_level": "INFO",
    "disable_anonymous_signature": true,
    "disable_update_check": true,
    "leave_on_terminate": true,
    "dns_config": {
        "allow_stale": true,
        "max_stale": "1s"
    }
}
