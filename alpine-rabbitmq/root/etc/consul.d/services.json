{
    "services": [
        {
            "name": "rabbitmq",
            "port": 5672,
            "checks": [
                {
                    "tcp": "127.0.0.1:5672",
                    "interval": "15s",
                    "timeout": "4s"
                }
            ]
        },
        {
            "name": "rabbitmq-management",
            "port": 15672,
            "checks": [
                {
                    "tcp": "127.0.0.1:15672",
                    "interval": "15s",
                    "timeout": "4s"
                }
            ]
        },
        {
            "name": "rabbitmq-stomp",
            "port": 61613,
            "checks": [
                {
                    "tcp": "127.0.0.1:61613",
                    "interval": "15s",
                    "timeout": "4s"
                }
            ]
        },
        {
            "name": "rabbitmq-mqtt",
            "port": 1883,
            "checks": [
                {
                    "tcp": "127.0.0.1:1883",
                    "interval": "15s",
                    "timeout": "4s"
                }
            ]
        }
    ]
}
